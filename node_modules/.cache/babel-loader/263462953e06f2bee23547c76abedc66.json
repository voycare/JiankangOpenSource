CA,QAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA;AAAA,UAAA,OAAA,EAAA,GAAA,CAAA;AAAA,SAAA;AACA,QAAA,IAAA,CAAA,SAAA;AACA,OAHA;AAIA,KAlBA;AAmBA,IAAA,gBAnBA,4BAmBA,CAnBA,EAmBA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,UAAA,IAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,UAAA,SAAA,GAAA,IAAA,QAAA,EAAA;AACA,MAAA,SAAA,CAAA,MAAA,CAAA,OAAA,EAAA,IAAA;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,sBAAA,CAAA,IAAA,CAAA,EAAA,EAAA,SAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA;AAAA,UAAA,OAAA,EAAA,GAAA,CAAA;AAAA,SAAA;AACA,OAFA;AAGA,KA5BA;AA6BA,IAAA,UA7BA,wBA6BA;AACA,UAAA,IAAA,GAAA,IAAA;AAEA,MAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA;AAjCA;AAbA,CAAA","sourcesContent":["<template>\n  <div class=\"content\">\n    <p class=\"title title-22 pb-5\">Gallery</p>\n\n    <p>Please upload photos no more than 2MB each, JPG format only.</p>\n\n    <div class=\"clinic--gallery\">\n      <div class=\"gallery--item\" v-for=\"photo in photos\" :key=\"photo.id\">\n        <img :src=\"VUE_APP_BASE_URL + photo.path\">\n        <a href=\"javascript:void(0)\" @click=\"onRemovePhoto(photo)\">Delete</a>\n      </div>\n      <a href=\"javascript:void(0)\" @click=\"onAddPhoto\" class=\"gallery--item\">\n        <i class=\"fa fa-plus\"></i>\n        <input @change=\"onAfterPickPhoto\" accept=\"image/*\" capture=\"environment\" ref=\"file\" style=\"display: none\"\n               type=\"file\">\n      </a>\n    </div>\n  </div>\n</template>\n<script>\n  import rf from \"../../api/RequestFactory\"\n\n  export default {\n    data() {\n      return {\n        id: null,\n        photos: []\n      }\n    },\n    mounted() {\n      let self = this\n\n      self.id = self.$route.params.id\n      self.getDetail()\n    },\n    methods: {\n      getDetail() {\n        let self = this\n\n        rf.getRequest('AdminRequest').getClinicPhotos(self.id).then(res => {\n          self.photos = res.data\n        })\n      },\n      onRemovePhoto(photo) {\n        let self = this\n        var r = confirm(\"Are you want to remove this clinic?\");\n        if (!r)\n          return\n\n        rf.getRequest('AdminRequest').postClinicPhotoRemove(self.id, {photo_id: photo.id}).then(res => {\n          window.$store.dispatch('app/showModal', {message: res.message})\n          self.getDetail()\n        })\n      },\n      onAfterPickPhoto(e) {\n        let self = this\n        const file = e.target.files[0]\n        const form_data = new FormData();\n        form_data.append('image', file)\n\n        rf.getRequest('AdminRequest').postClinicPhotosUpload(self.id, form_data).then(res => {\n          window.$store.dispatch('app/showModal', {message: res.message})\n        })\n      },\n      onAddPhoto() {\n        let self = this\n\n        self.$refs.file.click()\n      }\n    }\n  }\n</script>\n<style>\n  .gallery--item {\n    background-color: #fff;\n    min-height: 80px;\n    width: 20%;\n    display: inline-block;\n    text-align: center;\n    line-height: 80px;\n    margin-right: 15px;\n  }\n</style>\n"],"sourceRoot":"src/views/admin"}]}