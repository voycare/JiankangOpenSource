re=\"environment\" ref=\"file\" style=\"display: none\" type=\"file\">\n        <img :src=\"VUE_APP_BASE_URL + user.avatar\" ref=\"avatar\" v-if=\"user.avatar\">\n        <img class=\"no-avatar\" ref=\"avatar\" v-else>\n        <button @click=\"chooseImage\" class=\"choose-image\">Edit</button>\n    </div>\n</template>\n\n<script>\n  export default {\n    props: ['user'],\n    data() {\n      return {\n        imgSrc: null\n      }\n    },\n    methods: {\n      takePhoto(e) {\n        let self = this\n        const file = e.target.files[0];\n        const reader = new FileReader();\n        reader.onload = (event) => {\n          self.imgSrc = event.target.result;\n          self.$refs.avatar.src = event.target.result;\n\n          self.$emit('onChangeAvatar', self.imgSrc)\n        };\n        reader.readAsDataURL(file);\n      },\n      chooseImage() {\n        this.$refs.file.click();\n      },\n    }\n  }\n</script>\n\n<style>\n    .avatar-user {\n        position: relative;\n    }\n\n    .avatar-user img {\n        width: 100%;\n        height: 170px;\n    }\n\n    .avatar-user button {\n        position: absolute;\n        right: 0;\n        bottom: 0;\n        width: 54px;\n        height: 31px;\n        background: #434343;\n        font-family: Poppins;\n        font-style: normal;\n        font-weight: normal;\n        font-size: 15px;\n        line-height: 22px;\n        border: none;\n        color: #FFFFFF;\n    }\n</style>\n"],"sourceRoot":"src/components"}]}