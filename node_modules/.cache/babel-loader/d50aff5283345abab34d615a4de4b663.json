AAA,IAAA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,iBAAA,CAAA,IAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA;AAEA,QAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OAJA;AAKA,KAfA;AAgBA,IAAA,SAhBA,uBAgBA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,iBAAA,CAAA,IAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA;AAEA,QAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OAJA;AAKA;AAvBA;AAfA,CAAA","sourcesContent":["<template>\n  <div >\n    <div class=\"row\">\n      <div class=\"col-sm-12\">\n          <BackTo :to=\"'/clinic-applicants'\" :text=\"'Back to Applicants'\" />\n      </div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-sm-12\">\n        <div style=\"display: flex\">\n          <div class=\"card summary\" v-if=\"clinic\">\n            <h2 class=\"title-20 text-center text-color mb-5\">Clinic Applicant Summary</h2>\n\n            <div class=\"row\">\n              <div class=\"col-sm-3 title-15\">\n                Provider Name:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                {{ clinic.name }}\n              </div>\n              <div class=\"col-sm-3 title-15\">\n                Type:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                {{ clinic.profile.type_clinic }}\n              </div>\n            </div>\n\n            <div class=\"row\">\n              <div class=\"col-sm-3 title-15\">\n                Email Address:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                {{ clinic.email }}\n              </div>\n              <div class=\"col-sm-3 title-15\">\n                Phone:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                {{ clinic.phone }}\n              </div>\n            </div>\n\n            <div class=\"row\">\n              <div class=\"col-sm-3 title-15\">\n                City:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                {{ clinic.address.city }}\n              </div>\n              <div class=\"col-sm-3 title-15\">\n                Country:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                {{ clinic.address.country }}\n              </div>\n            </div>\n\n            <div class=\"row\">\n              <div class=\"col-sm-3 title-15\">\n                Website:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                -\n              </div>\n              <div class=\"col-sm-3 title-15\">\n                Contact Person:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                {{clinic.name}}\n              </div>\n            </div>\n\n            <div class=\"row\">\n              <div class=\"col-sm-3 title-15\">\n                Title:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                {{clinic.profile.title}}\n              </div>\n              <div class=\"col-sm-3 title-15\">\n                Telemedicine:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                -\n              </div>\n            </div>\n\n            <div class=\"row\">\n              <div class=\"col-sm-3 title-15\">\n                Years in Business:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                -\n              </div>\n              <div class=\"col-sm-3 title-15\">\n                Languages Spoken:\n              </div>\n              <div class=\"col-sm-3 desc-15\">\n                <span class=\"d-block\" v-for=\"item in clinic.languages\" :key=\"item.id\">\n                  {{item.language}}\n                </span>\n              </div>\n            </div>\n\n            <div class=\"pt-5\">\n              <div class=\"row\">\n                <div class=\"col-sm-6 text-center\">\n                  <button @click=\"onApprove\" class=\"btn btn-success rounded-pill\">Approve</button>\n                </div>\n\n                <div class=\"col-sm-6 text-center\">\n                  <button @click=\"onDecline\" class=\"btn btn-danger rounded-pill\">Decline</button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\n  import rf from \"../../api/RequestFactory\";\n  import Const from \"./../../utils/const\"\n  import BackTo from '../../components/BackTo'\n\n  export default {\n    components:{BackTo},\n    data() {\n      return {\n        Const,\n        clinic: null,\n        id: null\n      }\n    },\n    mounted() {\n      let self = this\n      self.id = parseInt(this.$route.params.id)\n\n      self.onRefresh()\n    },\n    methods: {\n      onRefresh() {\n        let self = this\n\n        rf.getRequest('AdminRequest').getClinic(self.id).then((res) => {\n          self.clinic = res.data;\n        });\n      },\n      onApprove() {\n        let self = this\n        rf.getRequest('AdminRequest').postClinicApprove(self.id).then(res => {\n          self.clinic = res.data\n\n          self.$router.push({path: '/clinic-applicants'})\n        })\n      },\n      onDecline() {\n        let self = this\n        rf.getRequest('AdminRequest').postClinicDecline(self.id).then(res => {\n          self.clinic = res.data\n\n          self.$router.push({path: '/clinic-applicants'})\n        })\n      }\n    }\n  }\n</script>\n<style scoped>\n.summary{\n  padding: 40px 90px;\n  width: 937px;\n  height: 655px;\n  border: 1px solid #CBCBCB;\n  border-radius: 5px;\n}\n.row{\n  margin-bottom: 20px;\n}\n.page-title, .title-15, .title-20, .btn{\n  color: #020644;\n}\nbutton{\n  width: 180px;\n}\n</style>\n"],"sourceRoot":"src/views/admin"}]}