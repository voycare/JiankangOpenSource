if (res.data) {\n          const {appointments, message, chat, call} = res.data.setting\n          self.appointments = appointments\n          self.message = message\n          self.chat = chat\n          self.call = call\n        }\n      });\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/views/clients"}]}