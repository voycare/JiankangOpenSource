A,UAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,IAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA;AACA,QAAA,IAAA,CAAA,WAAA,GAAA,IAAA;AACA,OAJA;AAKA,KAbA;AAcA,IAAA,SAdA,qBAcA,EAdA,EAcA;AACA,UAAA,IAAA,GAAA,IAAA;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,iBAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA;AAAA,UAAA,OAAA,EAAA,GAAA,CAAA;AAAA,SAAA;AACA,QAAA,IAAA,CAAA,UAAA,CAAA,IAAA,CAAA,WAAA;AACA,OAHA;AAIA,KArBA;AAsBA,IAAA,SAtBA,qBAsBA,EAtBA,EAsBA;AACA,UAAA,IAAA,GAAA,IAAA;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,iBAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA;AAAA,UAAA,OAAA,EAAA,GAAA,CAAA;AAAA,SAAA;AACA,QAAA,IAAA,CAAA,UAAA,CAAA,IAAA,CAAA,WAAA;AACA,OAHA;AAIA;AA7BA;AArBA,CAAA","sourcesContent":["<template>\n  <div class=\"content\" id=\"review_qage\">\n    <div class=\"row\">\n      <div class=\"col-sm-6 pl-0\">\n        <p class=\"title title-22\">Reviews</p>\n      </div>\n    </div>\n\n    <!-- <div class=\"list-content row\">\n      <div class=\"col-sm-12 form-inline mb-3\">\n        <button @click=\"show_popover_filter = !show_popover_filter\" class=\"btn btn-white btn-filter\" id=\"btn__popover_filter\"><span class=\"fa fa-filter\"></span></button>\n      </div>\n    </div> -->\n\n    <!-- <b-popover :show.sync=\"show_popover_filter\" placement=\"bottomright\" target=\"btn__popover_filter\">\n      <div class=\"row\">\n        <div class=\"col-sm-12\">\n          <div class=\"form-group\">\n          </div>\n        </div>\n      </div>\n      <div class=\"row\">\n        <div class=\"col-sm-6\">\n        </div>\n        <div class=\"col-sm-6\">\n        </div>\n      </div>\n    </b-popover> -->\n\n    <div class=\"row\">\n      <div class=\"table-responsive\">\n        <table class=\"table table-striped custom-table table--appt\">\n          <thead>\n          <tr>\n            <th>No.</th>\n            <th>Client</th>\n            <th>Country</th>\n            <th>Clinic</th>\n            <th>Transaction Date</th>\n            <th>Treatment</th>\n            <th>Rating</th>\n            <th>Actions</th>\n          </tr>\n          </thead>\n          <tbody>\n          <tr v-for=\"review in reviews\" :key=\"review.id\">\n            <td>{{ review.appointment_id | convertAppointmentID}}\n            </td>\n            <td>{{ review.client ? review.client.name : '' }}</td>\n            <td>{{ (review.client && review.client.address) ? review.client.address.country : '' }}</td>\n            <td>{{ review.clinic ? review.clinic.name : '' }}</td>\n            <td>{{ review.appointment.date | convertTimeSecondToDate }}</td>\n            <td>{{ appt_treatments[review.appointment.treatment_id] }}</td>\n            <td>{{ review.star }}</td>\n            <td style=\"position:relative\" class=\"button-table-review\">\n              <router-link class=\"\" :to=\"'/reviews/' + review.id\">\n                <button class=\"btn btn-light btn-rounded border-secondary mr-2\">\n                  <span>View</span>\n                </button>\n              </router-link>\n              <button class=\"btn btn-success btn-rounded mr-2\" @click=\"onApprove(review.id)\">Approve</button>\n              <button class=\"btn btn-danger btn-rounded\" @click=\"onDecline(review.id)\">Decline</button>\n            </td>\n          </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n\n    <div class=\"page\">\n      <Pagination :current-page=\"currentPage\" :total-page=\"totalPage\"\n                  @pageData=\"getListing\"></Pagination>\n    </div>\n  </div>\n</template>\n<script>\n  import {mapGetters} from 'vuex'\n  import Pagination from \"../../common/Pagination\"\n  import rf from \"../../api/RequestFactory\"\n\n  export default {\n    components: {\n      Pagination,\n    },\n    computed: {\n      ...mapGetters(['appt_treatments'])\n    },\n    data() {\n      return {\n        show_popover_filter: false,\n        currentPage: 1,\n        sortCreated: 'desc',\n        totalPage: 1,\n        reviews: [],\n      }\n    },\n    mounted() {\n      let self = this\n      self.$store.dispatch('getAppointmentMetas')\n      self.getListing(self.currentPage)\n    },\n    methods: {\n      getListing(page) {\n        let self = this\n        let params = {\n          page: page,\n          sort_created: this.sortCreated\n        };\n\n        rf.getRequest('AdminRequest').getReviews(params).then(res => {\n          self.reviews = res.data.datas\n          self.totalPage = res.data.total_page\n          self.currentPage = page\n        })\n      },\n      onApprove(id) {\n        let self = this\n\n        rf.getRequest('AdminRequest').postReviewApprove(id).then(res => {\n          self.$store.dispatch('app/showModal', {message: res.message})\n          self.getListing(self.currentPage)\n        })\n      },\n      onDecline(id) {\n        let self = this\n\n        rf.getRequest('AdminRequest').postReviewDecline(id).then(res => {\n          self.$store.dispatch('app/showModal', {message: res.message})\n          self.getListing(self.currentPage)\n        })\n      }\n    }\n  }\n</script>\n<style>\n  #review_qage table th {\n    width: 43px;\n    height: 21px;\n    font-weight: bold;\n    font-size: 14px;\n    line-height: 21px;\n    color: #434343;\n  }\n\n  .button-table-review button {\n    margin: auto;\n    padding: 0px;\n    width: 55px;\n    height: 20px;\n    font-size: 9px;\n    color: #000000;\n  }\n\n  .button-table-review .btn-success {\n    background: #93E9BE;\n  }\n\n  .button-table-review .btn-danger {\n    background: #FF0000;\n  }\n</style>\n"],"sourceRoot":"src/views/admin"}]}