,EAAA;AACA,aAAA,KAAA,CAAA,KAAA,GAAA,uBAAA;AACA,QAAA,MAAA,GAAA,KAAA;AACA,OAHA,MAGA;AACA,aAAA,KAAA,CAAA,KAAA,GAAA,EAAA;AACA;;AACA,UAAA,KAAA,KAAA,CAAA,KAAA,IAAA,CAAA,eAAA,IAAA,CAAA,KAAA,KAAA,CAAA,KAAA,CAAA,EAAA;AACA,aAAA,KAAA,CAAA,KAAA,GAAA,eAAA;AACA,QAAA,MAAA,GAAA,KAAA;AACA;;AACA,aAAA,MAAA;AACA,KAdA;AAeA,IAAA,oBAAA,EAAA,gCAAA;AACA,UAAA,IAAA,GAAA,IAAA;;AACA,UAAA,CAAA,KAAA,aAAA,EAAA,EAAA;AACA,eAAA,IAAA;AACA;;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,aAAA,EAAA,kBAAA,CAAA,IAAA,CAAA,KAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,MAAA;AACA,eAAA,GAAA;AAAA;AACA,cAAA,IAAA,CAAA,SAAA,GAAA,0CAAA;AACA;AACA;AAJA;AAMA,OAPA;AAQA,KA7BA;AA8BA,IAAA,WAAA,EAAA,uBAAA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,IAAA,CAAA,SAAA,GAAA,mCAAA;AACA;AAjCA;AAxBA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"mt-5 mb-4 text-center\">\n    </div>\n    <div class=\"row\">\n      <div class=\"col-sm-2\"></div>\n      <div class=\"col-sm-12 col-sm-8 vertical-center\">\n        <div class=\"card\">\n          <div class=\"card-header text-center\">\n            Forgot Password\n          </div>\n          <div class=\"card-body\">\n            <div class=\"row\">\n              <div class=\"col-sm-3\"></div>\n              <div class=\"col-sm-6\">\n                <form class=\"form-material\" method=\"post\">\n                  <div class=\"text-center mb-4\">\n                    <span\n                        class=\"desc-18\">Please enter your email and we will send you a link to reset your password.</span>\n                  </div>\n                  <div class=\"mb-4\">\n                    <input class=\"form-control\" placeholder=\"Email Address\" type=\"email\"\n                           v-model=\"input.email\">\n                    <span class=\"form-bar\"></span>\n                    <small class=\"text-danger text-sm-left\"\n                           style=\"display: block\">{{error.email}}\n                    </small>\n                  </div>\n\n                  <div class=\"text-center mb-5\">\n                    <button @click.prevent=\"requestResetPassword\"\n                            class=\"btn btn-pink account-btn btn-block\"\n                            type=\"submit\">\n                      Reset\n                    </button>\n                  </div>\n\n                  <div class=\"mt-5 mb-5 text-center desc-16\" v-if=\"error_msg\">{{ error_msg }}</div>\n\n                  <div class=\"text-center mb-4\">\n                    <router-link class=\"pink text-link title-15\" to=\"/login\">Back to Sign In\n                    </router-link>\n                  </div>\n                </form>\n              </div>\n              <div class=\"col-sm-3\"></div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"col-sm-2\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import API from \"./../utils/api\"\n  import utils from \"./../utils/utils\"\n  import {appHelper} from \"./../utils/appHelper\"\n  import rf from '../api/RequestFactory'\n\n  export default {\n    name: \"UserLogin\",\n    data() {\n      return {\n        error_msg: null,\n        input: {\n          email: \"\",\n        },\n        error: {\n          email: \"\",\n        }\n      }\n    },\n    mounted() {\n      if (utils.getLocalUser() && appHelper.isClient()) {\n        window.location.href = \"/\";\n      }\n      if (utils.getLocalAdmin() && appHelper.isAdmin()) {\n        window.location.href = \"/\";\n      }\n      if (utils.getLocalClinic() && appHelper.isClinic()) {\n        window.location.href = \"/\";\n      }\n    },\n    methods: {\n      validateInput: function () {\n        let result = true;\n        if (!this.input.email) {\n          this.error.email = \"email cannot be empty\";\n          result = false;\n        } else {\n          this.error.email = \"\";\n        }\n        if (this.input.email && !/\\S+@\\S+\\.\\S+/.test(this.input.email)) {\n          this.error.email = \"email invalid\";\n          result = false;\n        }\n        return result;\n      },\n      requestResetPassword: function () {\n        let self = this\n        if (!this.validateInput()) {\n          return null;\n        }\n\n        rf.getRequest('UserRequest').postForgetPassword(self.input).then(res => {\n          switch (res.status) {\n            case 200: {\n              self.error_msg = 'please check your email for new password'\n              return;\n            }\n          }\n        })\n      },\n      loginWechat: function () {\n        let self = this\n        self.error_msg = 'this function will be coming soon'\n      }\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/views"}]}