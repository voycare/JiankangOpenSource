AA,GAAA,IAAA;AACA,OAJA;AAKA,KAbA;AAcA,IAAA,SAdA,qBAcA,EAdA,EAcA;AACA,UAAA,IAAA,GAAA,IAAA;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,iBAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,UAAA,CAAA,IAAA,CAAA,YAAA;AACA,OAFA;AAGA,KApBA;AAqBA,IAAA,SArBA,qBAqBA,EArBA,EAqBA;AACA,UAAA,IAAA,GAAA,IAAA;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,iBAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,UAAA,CAAA,IAAA,CAAA,YAAA;AACA,OAFA;AAGA;AA3BA;AAlBA,CAAA","sourcesContent":["<template>\n  <div id=\"review_qage\">\n    <div class=\"row\">\n      <div class=\"col-sm-6 pl-0\">\n        <p class=\"title title-22\">Reviews</p>\n      </div>\n    </div>\n\n    <ReviewTable :reviews=\"reviews\" @onApprove=\"onApprove\" @onDecline=\"onDecline\"></ReviewTable>\n\n    <div class=\"page\">\n      <Pagination :current-page=\"current_page\" :total-page=\"total_page\"\n                  @pageData=\"getListing\"></Pagination>\n    </div>\n  </div>\n</template>\n<script>\n  import Pagination from \"../../common/Pagination\"\n  import rf from \"../../api/RequestFactory\"\n  import ReviewTable from '../../components/ReviewTable'\n\n  export default {\n    components: {\n      Pagination,\n      ReviewTable\n    },\n    data() {\n      return {\n        show_popover_filter: false,\n        current_page: 1,\n        sort_created: 'desc',\n        total_page: 1,\n        reviews: [],\n      }\n    },\n    mounted() {\n      let self = this\n      self.getListing(self.current_page)\n    },\n    methods: {\n      getListing(page) {\n        let self = this\n        let params = {\n          page: page,\n          sort_created: this.sort_created\n        };\n\n        rf.getRequest('AdminRequest').getReviews(params).then(res => {\n          self.reviews = res.data.datas\n          self.total_page = res.data.total_page\n          self.current_page = page\n        })\n      },\n      onApprove(id) {\n        let self = this\n\n        rf.getRequest('AdminRequest').postReviewApprove(id).then(res => {\n          self.getListing(self.current_page)\n        })\n      },\n      onDecline(id) {\n        let self = this\n\n        rf.getRequest('AdminRequest').postReviewDecline(id).then(res => {\n          self.getListing(self.current_page)\n        })\n      }\n    }\n  }\n</script>\n<style>\n  #review_qage table th {\n    width: 43px;\n    height: 21px;\n    font-weight: bold;\n    font-size: 14px;\n    line-height: 21px;\n    color: #434343;\n  }\n\n  .button-table-review button {\n    margin: auto;\n    padding: 0px;\n    width: 55px;\n    height: 20px;\n    font-size: 9px;\n    color: #000000;\n  }\n\n  .button-table-review .btn-success {\n    background: #93E9BE;\n  }\n\n  .button-table-review .btn-danger {\n    background: #FF0000;\n  }\n</style>\n"],"sourceRoot":"src/views/admin"}]}