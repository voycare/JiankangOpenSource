A,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AAAA;;AACA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,IAAA,CAAA,YAAA,GAAA,IAAA;AACA,UAAA,MAAA,GAAA;AACA,QAAA,IAAA,EAAA,IADA;AAEA,QAAA,KAAA,EAAA,KAAA,SAFA;AAGA,QAAA,YAAA,EAAA,IAAA,CAAA,YAHA;AAIA,QAAA,EAAA,EAAA,IAAA,CAAA,MAAA,CAAA,EAJA;AAKA,QAAA,IAAA,EAAA,IAAA,CAAA,MAAA,CAAA,IALA;AAMA,QAAA,EAAA,EAAA,IAAA,CAAA,MAAA,CAAA;AANA,OAAA;;AAQA,UAAA,IAAA,CAAA,SAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA;AACA,OAFA,MAEA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA;AACA;;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,aAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA;AACA,OAHA;AAIA,KAtBA;AAuBA,IAAA,WAvBA,yBAuBA;AACA,WAAA,YAAA,GAAA,CAAA;AACA,WAAA,UAAA,CAAA,CAAA;AACA;AA1BA;AA3BA,CAAA","sourcesContent":["<template>\n  <div>\n    <p class=\"title title-22 title-color\">\n      Payments <i class=\"fa fa-arrow-right\" aria-hidden=\"true\"></i> Clinic Statement\n    </p>\n\n    <div class=\"row\">\n      <div class=\"col-sm-3\">\n        <input type=\"text\" class=\"form-control\" v-model=\"search.id\" placeholder=\"Clinic\">\n      </div>\n      <div class=\"col-sm-3\">\n        <date-picker\n          style=\"width: 100%;\"\n          v-model=\"search.from\"\n          value-type=\"timestamp\"\n          placeholder=\"From\"\n        ></date-picker>\n      </div>\n      <div class=\"col-sm-3\">\n        <date-picker\n          style=\"width: 100%;\"\n          v-model=\"search.to\"\n          value-type=\"timestamp\"\n          placeholder=\"To\"\n        ></date-picker>\n      </div>\n      <div class=\"col-sm-3\">\n        <button class=\"btn btn-pink btn-block btn-rounded\" @click=\"getListing\">Search</button>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-sm-9\">\n        <b-form-select size=\"sm\" class=\"\" v-model=\"pageLimit\" style=\"width: 60px;\"\n            :options=\"limitOptions\" @change=\"limitChange\"></b-form-select>\n      </div>\n      <div class=\"col-sm-3 text-right\">\n        <router-link to=\"/statements-add\" class=\"btn btn-dark btn-block btn-rounded\">\n          Create Statement</router-link>\n      </div>\n    </div>\n\n    <div class=\"mt-2\">\n      <div class=\"row\">\n        <div class=\"col-md-12\">\n          <div class=\"table-responsive\">\n            <table class=\"m-t-25 table table-striped custom-table\">\n              <thead>\n              <tr>\n                <th>Statement No.</th>\n                <th>Clinic</th>\n                <th>Period</th>\n                <th>Amount</th>\n                <th>Status</th>\n                <th></th>\n              </tr>\n              </thead>\n              <tbody>\n              <tr :key=\"statement.id\" v-for=\"statement in statements\">\n                <td>\n                  <router-link :to=\"'/statements/' + statement.id\">\n                    <span class=\"text-blue desc-12\">{{ statement.id | convertHeraID }}</span>\n                  </router-link>\n                </td>\n                <td>{{ statement.to.name }}</td>\n                <td>{{ statement.sale_period | convertTimeSecondToDate }}</td>\n                <td>{{ statement.net }}</td>\n                <td>\n                  <p :class=\"Const.STATUS.intToPaymentStatus(statement.status)\" class=\"alert appt--status\">\n                    {{Const.STATUS.paymentStatusString( statement.status )}}\n                  </p>\n                </td>\n                <td>\n                  <router-link :to=\"'/statements/' + statement.id\">\n                    <i class=\"fa fa-pencil\"></i> Edit\n                  </router-link>\n                </td>\n              </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"page\">\n      <Pagination :current-page=\"current_page\" :total-page=\"total_page\"\n                  @pageData=\"getListing\"></Pagination>\n    </div>\n  </div>\n</template>\n\n<script>\n  import Const from \"./../../utils/const\"\n  import rf from \"../../api/RequestFactory\"\n  import Pagination from \"../../common/Pagination\"\n  import DatePicker from \"vue2-datepicker\";\n\n  export default {\n    components: {\n      Pagination, DatePicker\n    },\n    props: ['is_client'],\n    data() {\n      return {\n        Const,\n        statements: [],\n        page: null,\n        sort_created: 'desc',\n        current_page: 0,\n        total_page: 0,\n        id: null,\n        search: {\n          id: null,\n          from: null,\n          to: null\n        },\n        limitOptions: Const.PAGE_LIMIT_OPTIONS,\n        pageLimit: 10\n      }\n    },\n    mounted() {\n      let self = this\n      self.getListing(self.current_page)\n    },\n    methods: {\n      getListing(page) {\n        let self = this\n        self.current_page = page;\n        let params = {\n          page: page,\n          limit: this.pageLimit,\n          sort_created: self.sort_created,\n          id: self.search.id,\n          from: self.search.from,\n          to: self.search.to\n        }\n        if (self.is_client) {\n          params.type = 'client'\n        } else {\n          params.type = 'clinic'\n        }\n\n        rf.getRequest('AdminRequest').getStatements(params).then((res) => {\n          this.statements = res.data.datas;\n          this.total_page = res.data.total_page;\n        });\n      },\n      limitChange(){\n        this.current_page = 1\n        this.getListing(1)\n      }\n    }\n  }\n</script>\n\n<style></style>\n"],"sourceRoot":"src/views/admin"}]}