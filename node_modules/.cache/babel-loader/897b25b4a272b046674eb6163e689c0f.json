A,IAAA,EAAA;AACA,QAAA,IAAA,CAAA,YAAA,CAAA,eAAA,GAAA,IAAA;AACA,QAAA,IAAA,CAAA,YAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,IAAA,CAAA,YAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,IAAA,CAAA,YAAA,CAAA,aAAA,GAAA,IAAA;AACA,QAAA,IAAA,CAAA,YAAA,CAAA,eAAA,GAAA,IAAA;AACA,OANA,MAMA;AACA,QAAA,IAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA;AACA;AACA,KAZA;AAaA,IAAA,kBAbA,gCAaA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,YAAA;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,aAAA,EAAA,yBAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,WAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA;AAAA,YAAA,OAAA,EAAA,GAAA,CAAA;AAAA,WAAA;AACA;AACA,OALA;AAMA;AAvBA;AAzBA,CAAA","sourcesContent":["<template>\n    <div class=\"container pt-5\">\n        <div class=\"row\">\n            <div class=\"col-sm-2\">\n\n            </div>\n            <div class=\"col-sm-8\">\n                <h2 class=\"title-22 pb-4\" style=\"color: #020644\">Basic Settings</h2>\n                <div class=\"row\">\n                    <div class=\"col-sm-6\">\n                        <div class=\"country-infor\">\n                            <span>Default Country</span>\n                            <select v-model=\"localization.default_country\" class=\"hera-control\">\n                                <option disabled hidden value=\"\">Select Country</option>\n                                <option v-for=\"item in countries\" :value=\"item.id\" :key=\"item.id\">{{item.name}}\n                                </option>\n                            </select>\n                        </div>\n                    </div>\n                    <div class=\"col-sm-6\">\n                    </div>\n                </div>\n\n                <div class=\"row m-t-25\">\n                    <div class=\"col-sm-6\">\n                        <span>Timezone</span>\n                        <select v-model=\"localization.timezone\" class=\"hera-control\">\n                            <option disabled hidden value=\"\">Select Timezone</option>\n                            <option v-for=\"(name, id) in settings.timezones\" :value=\"id\" :key=\"id\">{{name}}</option>\n                        </select>\n                    </div>\n\n                    <div class=\"col-sm-6\">\n                        <span>Language</span>\n                        <select v-model=\"localization.language\" class=\"hera-control\">\n                            <option disabled hidden value=\"\">Select Language</option>\n                            <option v-for=\"(name, id) in settings.languages\" :value=\"id\" :key=\"id\">{{name}}\n                            </option>\n                        </select>\n                    </div>\n                </div>\n\n                <div class=\"row m-t-25\">\n                    <div class=\"col-sm-6\">\n                        <span>Currency Code</span>\n                        <select v-model=\"localization.currency_code\" class=\"hera-control\">\n                            <option disabled hidden value=\"\">Select Language</option>\n                            <option v-for=\"(name, id) in settings.currency_codes\" :value=\"id\" :key=\"id\">\n                                {{name}}\n                            </option>\n                        </select>\n                    </div>\n\n                    <div class=\"col-sm-6\">\n                        <span>Currency Symbol</span>\n                        <input type=\"text\" class=\"hera-control text-right\" v-model=\"localization.currency_symbol\">\n                    </div>\n                </div>\n\n                <div class=\"row\">\n                    <div class=\"col-sm-4\"></div>\n                    <div class=\"col-sm-4\">\n                        <button class=\"btn  btnSave\"  @click=\"updateLocalization\">Save</button>\n                    </div>\n                    <div class=\"col-sm-4\"></div>\n                </div>\n            </div>\n            <div class=\"col-sm-2\">\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n  import rf from \"../../api/RequestFactory\";\n  import {mapGetters} from 'vuex'\n\n  export default {\n    computed: {\n      ...mapGetters(['countries', 'settings'])\n    },\n    data() {\n      return {\n        localization: {\n          default_country: null,\n          timezone: null,\n          language: null,\n          currency_code: null,\n          currency_symbol: null\n        }\n      }\n    },\n    mounted() {\n      let self = this\n\n      self.$store.dispatch('getCountries')\n      self.$store.dispatch('getSettings')\n\n      rf.getRequest('UserRequest').getSettingLocalization().then(res => {\n        self._updateData(res)\n      });\n    },\n    methods: {\n      _updateData(res) {\n        let self = this\n        if (!res.data) {\n          self.localization.default_country = null\n          self.localization.timezone = null\n          self.localization.language = null\n          self.localization.currency_code = null\n          self.localization.currency_symbol = null\n        } else {\n          self.localization = res.data;\n        }\n      },\n      updateLocalization() {\n        let self = this\n        let params = self.localization\n\n        rf.getRequest('UserRequest').updateSettingLocalization(params).then(res => {\n          self._updateData(res)\n          if(res.data) {\n            self.$store.dispatch('app/showModal', {message: res.message})\n          }\n        });\n      }\n    }\n  }\n</script>\n<style>\n\n</style>"],"sourceRoot":"src/views/clients"}]}