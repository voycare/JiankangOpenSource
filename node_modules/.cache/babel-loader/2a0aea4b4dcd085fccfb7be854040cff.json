;AACA,UAAA,IAAA,EAAA,KAAA;AADA,SAAA;AAPA,OAAA,CAAA,CAUA,IAVA,CAUA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,IAAA,EAAA;AAAA,OAVA,EAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA,CAAA,OAAA,CAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA;;AACA,YAAA,KAAA,CAAA,IAAA,IAAA,GAAA,CAAA,IAAA,CAAA,UAAA,EAAA;AACA,UAAA,KAAA,CAAA,SAAA,CAAA,OAAA,CAAA,OAAA,GAAA,KAAA;AACA;;AACA,QAAA,KAAA,CAAA,IAAA;AACA,OAhBA,EAgBA,KAhBA,CAgBA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAlBA;AAmBA,KArBA;AAsBA,IAAA,YAtBA,wBAsBA,EAtBA,EAsBA;AACA,MAAA,KAAA,CAAA,aAAA,EAAA,CAAA;AACA;AAxBA;AAzBA,CAAA","sourcesContent":["<template>\n    <div class=\"content\">\n        <div class=\"row\">\n            <div class=\"col-sm-4 col-3\">\n                <h4 class=\"page-title\">clinics</h4>\n            </div>\n            <div class=\"col-sm-8 col-9 text-right m-b-20\">\n                <a class=\"btn btn-primary btn-rounded float-right\" href=\"/admin/clinic/add\"><i class=\"fa fa-plus\"></i>\n                    Add\n                    clinic</a>\n            </div>\n        </div>\n        <div class=\"row clinic-grid\">\n            <div :key=\"index\" class=\"col-md-4 col-sm-4  col-lg-3\" style=\"margin-bottom: 10px\"\n                 v-for=\"(clinic, index) in clinics\">\n                <div class=\"profile-widget\">\n                    <div class=\"doctor-img\">\n                        <a @click.prevent=\"\" class=\"\" href=\"#\"><img :src=\"clinic.avatar\" alt=\"\" style=\"width: 100%\"></a>\n                    </div>\n                    <div class=\"dropdown profile-action\">\n                        <a aria-expanded=\"false\" class=\"action-icon dropdown-toggle\" data-toggle=\"dropdown\" href=\"#\"><i\n                                class=\"fa fa-ellipsis-v\"></i></a>\n                        <div class=\"dropdown-menu dropdown-menu-right\">\n                            <a class=\"dropdown-item\" href=\"edit-clinic.html\"><i class=\"fa fa-pencil m-r-5\"></i> Edit</a>\n                            <a @click.prevent=\"deleteclinic(clinic.id)\" class=\"dropdown-item\" data-target=\"#delete_clinic\"\n                               data-toggle=\"modal\" href=\"#\"><i class=\"fa fa-trash-o m-r-5\"></i>\n                                Delete</a>\n                        </div>\n                    </div>\n                    <h4 class=\"clinic-name text-ellipsis\"><a href=\"profile.html\">{{clinic.name}}</a></h4>\n                    <div class=\"doc-prof\">Gynecologist</div>\n                    <div class=\"user-country\">\n                        <i class=\"fa fa-map-marker\"></i> {{clinic.address}}\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-sm-12\">\n                <div class=\"see-all\">\n                    <a @click.prevent=\"getClinic()\" class=\"see-all-btn\" href=\"#\" v-if=\"component.btnLoad.display\">Load\n                        More</a>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n  import API from \"../utils/api\";\n  import Utils from \"../utils/utils\";\n\n  export default {\n    name: \"Adminclinic\",\n    data() {\n      return {\n        clinic: {\n          \"id\": 0,\n          \"name\": \"\",\n          \"email\": \"\",\n          \"address\": \"\",\n          \"avatar\": \"\",\n          \"phone\": \"\",\n          \"country\": \"\"\n        },\n        clinics: [],\n        page: 1,\n        component: {\n          btnLoad: {\n            display: true\n          }\n        }\n      }\n    },\n    mounted() {\n      this.getClinic(1)\n    },\n    methods: {\n      getClinic() {\n        fetch(API.getApi(API.ADMIN.CLINIC.LIST), {\n          method: 'post',\n          headers: {\n            'Accept': 'application/json',\n            'Content-Type': 'application/json',\n            authorization: Utils.getLocalAdmin().token\n          },\n          body: JSON.stringify({\n            page: this.page\n          })\n        }).then(res => res.json()).then(res => {\n          this.clinics = this.clinics.concat(res.data.datas);\n          if (this.page >= res.data.total_page) {\n            this.component.btnLoad.display = false;\n          }\n          this.page++;\n        }).catch(e => {\n          console.log(e)\n        });\n      },\n      deleteclinic(id) {\n        alert(\"delete: \" + id);\n      }\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/views"}]}