AA,CAAA,IAAA,CAAA,UAAA;AACA,OAHA;AAIA,KAZA;AAaA,IAAA,SAbA,qBAaA,EAbA,EAaA;AACA,UAAA,IAAA,GAAA,IAAA;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,iBAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA;AAAA,UAAA,OAAA,EAAA,GAAA,CAAA;AAAA,SAAA;AACA,QAAA,IAAA,CAAA,UAAA,CAAA,IAAA,CAAA,WAAA;AACA,OAHA;AAIA,KApBA;AAqBA,IAAA,SArBA,qBAqBA,EArBA,EAqBA;AACA,UAAA,IAAA,GAAA,IAAA;AAEA,MAAA,EAAA,CAAA,UAAA,CAAA,cAAA,EAAA,iBAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA;AAAA,UAAA,OAAA,EAAA,GAAA,CAAA;AAAA,SAAA;AACA,QAAA,IAAA,CAAA,UAAA,CAAA,IAAA,CAAA,WAAA;AACA,OAHA;AAIA;AA5BA;AAjBA,CAAA","sourcesContent":["<template>\n  <div class=\"content\">\n    <div class=\"row\">\n      <div class=\"col-sm-6\">\n        <p class=\"title title-22\">Reviews</p>\n      </div>\n    </div>\n\n    <div class=\"row list-content\">\n      <div class=\"col-sm-12 form-inline mb-3\">\n        <button @click=\"show_popover_filter = !show_popover_filter\" class=\"btn btn-white btn-filter\" id=\"btn__popover_filter\"><span class=\"fa fa-filter\"></span></button>\n      </div>\n    </div>\n\n    <b-popover :show.sync=\"show_popover_filter\" placement=\"bottomright\" target=\"btn__popover_filter\">\n      <div class=\"row\">\n        <div class=\"col-sm-12\">\n          <div class=\"form-group\">\n          </div>\n        </div>\n      </div>\n      <div class=\"row\">\n        <div class=\"col-sm-6\">\n        </div>\n        <div class=\"col-sm-6\">\n        </div>\n      </div>\n    </b-popover>\n\n    <div class=\"row list-content\">\n      <div class=\"table-responsive\">\n        <table class=\"table table-striped custom-table\">\n          <thead>\n          <tr>\n            <th>No</th>\n            <th>Client</th>\n            <th>Country</th>\n            <th>Clinic</th>\n            <th>Transaction Date</th>\n            <th>Treatment</th>\n            <th>Rating</th>\n            <th>Actions</th>\n          </tr>\n          </thead>\n          <tbody>\n          <tr v-for=\"review in reviews\" :key=\"review.id\">\n            <td>{{ review.appointment_id | convertAppointmentID}}\n            </td>\n            <td>{{ review.client ? review.client.name : '' }}</td>\n            <td>{{ review.client ? review.client.address.country : '' }}</td>\n            <td>{{ review.clinic ? review.clinic.name : '' }}</td>\n            <td>{{ review.transaction_id }}</td>\n            <td>{{ review.treatment.type }}</td>\n            <td>{{ review.star }}</td>\n            <td style=\"position:relative\">\n              <router-link class=\"btn btn-default btn-rounded\" :to=\"'/reviews/' + review.id\">\n                <span>View</span>\n              </router-link>\n              <button class=\"btn btn-success btn-rounded mr-2\" @click=\"onApprove(review.id)\">Approve</button>\n              <button class=\"btn btn-danger btn-rounded\" @click=\"onDecline(review.id)\">Decline</button>\n            </td>\n          </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n\n    <div class=\"page\">\n      <Pagination :current-page=\"currentPage\" :total-page=\"totalPage\"\n                  @pageData=\"getListing\"></Pagination>\n    </div>\n  </div>\n</template>\n<script>\n  import Pagination from \"../../common/Pagination\"\n  import rf from \"../../api/RequestFactory\"\n\n  export default {\n    components: {\n      Pagination,\n    },\n    data() {\n      return {\n        show_popover_filter: false,\n        currentPage: 1,\n        sortCreated: 'desc',\n        totalPage: 1,\n        reviews: [],\n      }\n    },\n    mounted() {\n      let self = this\n      self.getListing(self.currentPage)\n    },\n    methods: {\n      getListing(page) {\n        let self = this\n        let params = {\n          page: page,\n          sort_created: this.sortCreated\n        };\n\n        rf.getRequest('AdminRequest').getReviews(params).then(res => {\n          self.reviews = res.data.datas\n          self.totalPage = res.data.total_page\n        })\n      },\n      onApprove(id) {\n        let self = this\n\n        rf.getRequest('AdminRequest').postReviewApprove(id).then(res => {\n          self.$store.dispatch('app/showModal', {message: res.message})\n          self.getListing(self.currentPage)\n        })\n      },\n      onDecline(id) {\n        let self = this\n\n        rf.getRequest('AdminRequest').postReviewDecline(id).then(res => {\n          self.$store.dispatch('app/showModal', {message: res.message})\n          self.getListing(self.currentPage)\n        })\n      }\n    }\n  }\n</script>\n<style>\n</style>\n"],"sourceRoot":"src/views/admin"}]}