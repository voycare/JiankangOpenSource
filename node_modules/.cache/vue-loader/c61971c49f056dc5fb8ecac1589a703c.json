To\n    },\n    data() {\n      return {\n        Const,\n        id: null,\n        news: null\n      }\n    },\n    mounted() {\n      let self = this\n\n      self.id = self.$route.params.id\n      if (self.id) {\n        self.getDetail()\n      }\n    },\n    methods: {\n      getDetail() {\n        let self = this\n\n        rf.getRequest('AdminRequest').getNewsDetail(self.id).then(res => {\n          self.news = res.data\n        })\n\n      },\n      onSave(news) {\n        let self = this\n        const params = {...news}\n        params.id = self.id\n\n        rf.getRequest('AdminRequest').postNewsStore(params).then(res => {\n          self.news = res.data\n        })\n      }\n    },\n  }\n</script>\n\n<style scoped>\n</style>\n"]}]}