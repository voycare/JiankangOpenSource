          <span>{{ item.date | convertTimeSecondToDate }}</span>\n                    <b-dropdown size=\"lg\" variant=\"link\" toggle-class=\"text-decoration-none\" no-caret>\n                        <template v-slot:button-content>\n                            <i class=\"fa fa-bars\"></i>\n                        </template>\n                        <b-dropdown-item href=\"#\"><i class=\"fa fa-trash\"></i>Delete</b-dropdown-item>\n                    </b-dropdown>\n                </div>\n            </li>\n            <li class=\"available--item\">\n                <button @click=\"onAddOnlineConsulation\" class=\"btn btn-default\">\n                    <i class=\"fa fa-plus\"></i>\n                </button>\n            </li>\n        </ul>\n\n        <p class=\"text-danger title-20\">\n            Step 2: Site visit appointments\n        </p>\n        <p>Please use below calendar to select the available time slots you can provide site visit appointment to\n            patients on the\n            Hera platform. For China clients, please consider time difference accordingly.</p>\n\n        <ul class=\"list-unstyled available--wrapper\">\n            <li class=\"available--item\">\n                <button @click=\"onAddSiteVisit\" class=\"btn btn-default\">\n                    <i class=\"fa fa-plus\"></i>\n                </button>\n            </li>\n        </ul>\n    </div>\n</template>\n\n<script>\n  import Const from \"./../../utils/const\"\n  import rf from \"../../api/RequestFactory\"\n\n  export default {\n    components: {},\n    data() {\n      return {\n        Const,\n        id: null,\n        online_consulations: [],\n        site_visits: []\n      }\n    },\n    methods: {\n      getDetail() {\n        let self = this\n\n        rf.getRequest('AdminRequest').getClinicAvailables(self.id).then(res => {\n          self.online_consulations = res.data.online_consulations\n          self.site_visits = res.data.site_visits\n        })\n      },\n      onAddOnlineConsulation() {\n        let self = this\n\n        self.online_consulations.push({\n          clinic_id: self.id,\n          type: 1,\n          date: (new Date()).getTime() / 1000,\n        })\n      },\n      onAddSiteVisit() {\n        let self = this\n\n        self.site_visits.push({\n          clinic_id: self.id,\n          type: 2,\n          date: (new Date()).getTime() / 1000,\n        })\n      }\n    },\n\n    mounted() {\n      let self = this\n\n      self.id = self.$route.params.id\n    }\n  }\n</script>\n\n<style scoped>\n    .available--wrapper {\n        background-color: #020644;\n        padding: 15px;\n    }\n\n    .available--item {\n        display: inline-block;\n        width: 145px;\n        height: 60px;\n        background-color: #fff;\n        vertical-align: middle;\n        text-align: center;\n        margin-left: 15px;\n        margin-right: 15px;\n    }\n</style>\n"]}]}