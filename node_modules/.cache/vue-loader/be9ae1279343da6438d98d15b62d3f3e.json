lse>\n          <template v-if=\"currentPage<totalPage-5\">\n            <li v-for=\"(page, pageIndex) in 10\" v-bind:key=\"pageIndex\"\n                :class=\"{active:setActive((page+currentPage-6),currentPage)}\">\n              <a v-on:click=\"getData((page+currentPage-6))\">\n                {{page+currentPage-6}}</a>\n            </li>\n          </template>\n          <template v-else>\n            <li v-for=\"(page, pageIndex) in 10\" v-bind:key=\"pageIndex\"\n                :class=\"{active:setActive((page+totalPage-10),currentPage)}\">\n              <a v-on:click=\"getData((page+totalPage-10))\">\n                {{page+totalPage-10}}\n              </a>\n            </li>\n          </template>\n        </template>\n        <li v-if=\"currentPage<totalPage\">\n          <a v-on:click=\"getData(currentPage+1)\">Next</a>\n        </li>\n      </template>\n    </div>\n  </div>\n</template>\n\n<script>\n  export default {\n    props: {\n      totalPage: {\n        type: Number,\n        require: true\n      },\n      currentPage: {\n        type: Number,\n        require: true\n      }\n    },\n    methods: {\n      setActive(page, currentPage) {\n        return page == currentPage;\n      },\n      getData(page) {\n        return this.$emit('pageData', page);\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  li {\n    color: #020644;\n    cursor: pointer;\n\n    a {\n      background-color: lightgrey;\n      margin-left: 5px;\n      border-radius: 5px;\n    }\n\n  }\n\n  li.active {\n    a {\n      background-color: #FFAFAE;\n      color: white;\n\n      &:hover {\n        background-color: #FFAFAE;\n        color: #020644 !important;\n      }\n    }\n  }\n</style>"]}]}