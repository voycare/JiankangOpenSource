eRoot":"src/views/admin","sourcesContent":["<template>\n    <div class=\"content\">\n        <p class=\"title title-22\">Appointments<i class=\"fa fa-arrow-right mr-3 ml-3\"></i>24 - Hour</p>\n        <div class=\"search\">\n            <!--<input v-model=\"name\" class=\"content-name\" type=\"text\" placeholder=\"Search\"-->\n            <!--@input=\"getDataAppointments\">-->\n            <!--<select v-model=\"country\" class=\"coutry-search\">-->\n            <!--<option disabled hidden value=\"\">Select Country</option>-->\n            <!--<option v-for=\"item in countries\" :value=\"item.name\" :key=\"item.id\">{{item.name}}</option>-->\n            <!--</select>-->\n\n        </div>\n\n        <div class=\"row list-content\">\n            <div class=\"col-sm-12 form-inline mb-3\">\n                <img src=\"../../assets/filter-icon.svg\" alt=\"\">\n                <select style=\"margin-left: 10px\" class=\"form-control\">\n                    <option value=\"10\">10</option>\n                    <option value=\"20\">20</option>\n                    <option value=\"50\">50</option>\n                </select>\n                <button style=\"color: #EB6271;\" class=\"btn btn-white m-l-30\" @click=\"onNotifyAll\">Notify All</button>\n            </div>\n\n            <AdminAppointmentListing :appoinments=\"appoinments\" @onSortCreated=\"onSortCreated\" :showNotify=\"true\" @onRefresh=\"getDataAppointments(currentPage)\"></AdminAppointmentListing>\n        </div>\n\n        <div class=\"page\">\n            <Pagination :total-page=\"totalPage\" :current-page=\"currentPage\"\n                        @pageData=\"getDataAppointments\"></Pagination>\n        </div>\n    </div>\n</template>\n\n<script>\n  import {mapGetters} from 'vuex'\n\n  import Const from \"./../../utils/const\"\n  import rf from \"../../api/RequestFactory\"\n  import Pagination from \"../../common/Pagination\"\n  import AdminAppointmentListing from '../../components/AdminAppointmentListing'\n\n  export default {\n    name: \"Appoinment\",\n    components: {\n      Pagination,\n      AdminAppointmentListing\n    },\n    data() {\n      return {\n        Const,\n        country: '',\n        appoinments: [],\n        currentPage: 1,\n        sortCreated: 'desc',\n        name: '',\n        totalPage: 1,\n      }\n    },\n    watch: {\n      country: function () {\n        this.getDataAppointments();\n      }\n    },\n    computed: {\n      ...mapGetters(['countries', 'appt_types', 'appt_status'])\n    },\n    methods: {\n      onNotifyAll() {\n        let self = this\n        let params = {\n          type: '24h'\n        }\n\n        rf.getRequest('AdminRequest').postAppointmentNotifyAll(params).then(res => {\n          self.getDataAppointments(self.currentPage)\n        })\n      },\n      getDataAppointments(page) {\n        this.currentPage = page;\n        let params = {\n          page: page,\n          sort_created: this.sortCreated,\n          // key: this.name,\n          country: this.country,\n          type: '24h'\n        };\n\n        rf.getRequest('AdminRequest').getAppointments(params).then((res) => {\n          this.appoinments = res.data.datas;\n          this.totalPage = res.data.total_page;\n        })\n      },\n      onSortCreated() {\n        this.getDataAppointments(1);\n      },\n    },\n\n    mounted() {\n      let self = this\n\n      self.$store.dispatch('getCountries')\n      self.$store.dispatch('getAppointmentMetas')\n      self.getDataAppointments(this.currentPage);\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n    .content {\n        padding: 30px 30px;\n\n        .page {\n            margin-top: 5px;\n        }\n\n        .search {\n            text-align: right;\n        }\n\n        .title {\n            font-family: Poppins;\n            font-style: normal;\n            font-weight: 500;\n            font-size: 22px;\n            line-height: 33px;\n            color: #020644;\n        }\n\n        .content-name {\n            box-sizing: border-box;\n            width: 200px;\n            height: 40px;\n            padding-left: 10px;\n            border: 0.5px solid #979797;\n            margin-right: 15px;\n            font-family: Poppins;\n            font-style: normal;\n            font-weight: normal;\n            font-size: 15px;\n            background: url('/assets/img/icon_search.png') no-repeat 170px;\n            background-color: #FFFFFF\n\n        }\n\n\n        .coutry-search {\n            width: 200px;\n            height: 40px;\n            padding-left: 10px;\n            background: #FFFFFF;\n            border: 0.5px solid #979797;\n            font-family: Poppins;\n            font-style: normal;\n            font-weight: normal;\n            font-size: 15px;\n        }\n\n        .list-content {\n            margin-top: 20px;\n\n            table {\n                border-collapse: separate;\n                border-spacing: 0px;\n            }\n\n            td {\n                border-top: none;\n                border-bottom: 1px #898989 solid;\n                border-left: none;\n                border-right: none;\n            }\n\n            th {\n                font-family: Poppins;\n                font-style: normal;\n                font-weight: bold;\n                font-size: 14px;\n                color: #434343;\n                border-top: 1.5px #898989 solid;\n                border-bottom: 1.5px #898989 solid;\n                border-left: none;\n                border-right: none;\n            }\n\n            th.date {\n                color: #EB6271;\n                cursor: pointer;\n\n                .title-date {\n                    margin-right: 6px;\n                }\n\n                .img-down {\n                    margin-right: 2px;\n                }\n            }\n\n            td {\n                font-family: Poppins;\n                font-style: normal;\n                font-size: 12px;\n                line-height: 18px;\n                color: #434343;\n            }\n\n            tr {\n                height: 45px;\n            }\n\n            .confirmed {\n                height: 22px;\n                margin: 0px !important;\n                text-align: center;\n                width: 83px;\n                background: rgba(147, 233, 190, 0.3);\n                border: 2px solid #93E9BE;\n                box-sizing: border-box;\n                border-radius: 5px;\n            }\n\n            .canceled {\n                height: 22px;\n                margin: 0px !important;\n                width: 83px;\n                text-align: center;\n                background: rgba(255, 0, 0, 0.5);\n                border: 2px solid #FF0000;\n                box-sizing: border-box;\n                border-radius: 5px;\n            }\n\n            .completed {\n                height: 22px;\n                margin: 0px !important;\n                text-align: center;\n                width: 83px;\n                padding: 1px 6px;\n                background: #FDD3D1;\n                border: 2px solid #FFAFAE;\n                box-sizing: border-box;\n                border-radius: 5px;\n            }\n\n            .pending {\n                height: 22px;\n                margin: 0px !important;\n                text-align: center;\n                width: 83px;\n                background: rgba(255, 207, 37, 0.5);\n                border: 2px solid #FFCF25;\n                box-sizing: border-box;\n                border-radius: 5px;\n            }\n\n            .review {\n                cursor: pointer;\n                height: 22px;\n                background: #EB6271;\n                border-radius: 5px;\n                color: white;\n                padding: 3px 10px;\n                margin-left: 6px;\n\n                &:hover {\n                    color: #1b8bf9;\n                }\n            }\n        }\n    }\n</style>"]}]}