 10\">\n              <a v-on:click=\"getData(page)\">{{page}}</a>\n            </li>\n          </template>\n          <template v-else>\n            <template v-if=\"currentPage<totalPage-5\">\n              <li :class=\"{active:setActive((page+currentPage-6),currentPage)}\" v-bind:key=\"pageIndex\"\n                  v-for=\"(page, pageIndex) in 10\">\n                <a v-on:click=\"getData((page+currentPage-6))\">\n                  {{page+currentPage-6}}</a>\n              </li>\n            </template>\n            <template v-else>\n              <li :class=\"{active:setActive((page+totalPage-10),currentPage)}\" v-bind:key=\"pageIndex\"\n                  v-for=\"(page, pageIndex) in 10\">\n                <a v-on:click=\"getData((page+totalPage-10))\">\n                  {{page+totalPage-10}}\n                </a>\n              </li>\n            </template>\n          </template>\n          <li v-if=\"currentPage<totalPage\">\n            <a v-on:click=\"getData(currentPage+1)\">Next</a>\n          </li>\n        </template>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  export default {\n    props: {\n      totalPage: {\n        type: Number,\n        require: true\n      },\n      currentPage: {\n        type: Number,\n        require: true\n      }\n    },\n    methods: {\n      setActive(page, currentPage) {\n        return page == currentPage;\n      },\n      getData(page) {\n        return this.$emit('pageData', page);\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .paginate--wrapper li {\n    color: #020644;\n    cursor: pointer;\n  }\n  .paginate--wrapper li a {\n    background-color: lightgrey;\n    margin-left: 5px;\n    border-radius: 5px;\n  }\n  .paginate--wrapper li.active a {\n    background-color: #EB6271;\n    color: white;\n  }\n  .paginate--wrapper li.active a:hover {\n    background-color: #FFAFAE;\n    color: #020644 !important;\n  }\n</style>\n"]}]}