               <div class=\"col-sm-4\">\n                            Online consulation (audio/video)\n                        </div>\n                        <div class=\"col-sm-2\">\n                            {{ service.duration }} min\n                        </div>\n                        <div class=\"col-sm-2\">\n                            <span>$</span>{{ service.price }}<span>USD</span>\n                        </div>\n                    </div>\n\n                    <div class=\"row\" v-if=\"service.type == 2\">\n                        <div class=\"col-sm-4\">\n                            Site visit appointment\n                        </div>\n                        <div class=\"col-sm-2\">\n                            {{ service.duration }} min\n                        </div>\n                        <div class=\"col-sm-2\">\n                            <span>$</span>{{ service.price }}<span>USD</span>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"card\" v-if=\"treatments.length\">\n            <div class=\"card-body\">\n                <div class=\"title-22\">\n                    Treatments\n                </div>\n\n                <div :key=\"treatment.id\" class=\"block--treatment\" v-for=\"treatment in treatments\">\n                    <div class=\"row\">\n                        <div class=\"col-sm-3\">\n                            {{ treatment.type }}\n                        </div>\n                        <div class=\"col-sm-3\">\n                            From $ {{ treatment.from }} USD\n                        </div>\n                    </div>\n                    <p class=\"desc-15\">{{ treatment.description }}</p>\n                </div>\n            </div>\n        </div>\n\n    </div>\n</template>\n\n<script>\n  import Const from \"./../../utils/const\"\n  import rf from \"../../api/RequestFactory\"\n\n  export default {\n    data() {\n      return {\n        Const,\n        id: null,\n        services: [],\n        treatments: []\n      }\n    },\n    methods: {\n      getDetail() {\n        let self = this\n\n        rf.getRequest('AdminRequest').getClinicServices(self.id).then(res => {\n          self.services = res.data.services\n          self.treatments = res.data.treatments\n        })\n      }\n    },\n    mounted() {\n      let self = this\n\n      self.id = self.$route.params.id\n      self.getDetail()\n    }\n  }\n</script>\n"]}]}