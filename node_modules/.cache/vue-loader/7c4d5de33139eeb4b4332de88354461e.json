                    <th v-if=\"disableView\">Actions</th>\n                    </tr>\n                    </thead>\n                    <tbody>\n                    <tr v-for=\"appt in appts\" v-bind:key=\"appt.id\">\n                        <td>\n                            <div style=\"width: 70px;height:40px;background-color: #c4c4c4;float:left;margin-right:20px\"></div>\n                            <h2 class=\"time-title title-15 pb-2\">\n                                Appointment with\n                            </h2>\n                            <div class=\"text-desc desc-11\">{{ appt.doctor.first_name }}&nbsp;{{ appt.doctor.last_name }}</div>\n                        </td>\n                        <td>\n                            <h2 class=\"time-title title-15\">\n                                Treatment\n                                <span>{{ appt.doctor.clinic }}</span>\n                            </h2>\n                        </td>\n                        <td>\n                            <h2 class=\"time-title title-15\">\n                                Appt. Date\n                                <span>{{appt.date|convertTimeSecondToDate}}</span>\n                            </h2>\n                        </td>\n                        <td>\n                            <h2 class=\"time-title title-15\">\n                                Appt. Time\n                                <span>{{appt.date|convertDateToTime}}</span>\n                            </h2>\n                        </td>\n                        <td class=\"text-center\">\n                            <p class=\"alert appt--status\" :class=\"Const.STATUS.intToStatusClass(appt.status)\">\n                                {{Const.STATUS.intToString( appt.status )}}\n                            </p>\n                        </td>\n                        <td class=\"text-center\" v-if=\"! disableView\">\n                            <router-link v-if=\"appt.status !== 1\" class=\"btn btn-round btn-primary\" :to=\"'appointment/' + appt.id\">View</router-link>\n                        </td>\n                    </tr>\n                    </tbody>\n                </table>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n  import Const from \"./../utils/const\"\n\n  export default {\n    props: {\n      title: {\n        type: String\n      },\n      appts: {\n        type: Array\n      },\n      disableView: {\n        type: Boolean,\n        default: false\n      }\n    },\n    data() {\n      return {\n        Const: Const,\n      }\n    }\n  }\n</script>\n\n<style>\n    .appt--status {\n        text-align: center;\n        font-style: normal;\n        font-weight: 500;\n        font-size: 12px;\n        line-height: 18px;\n        margin-bottom: 0px;\n        padding: 5px 12px;\n    }\n\n    .table--appointment td {\n        border-bottom: 0.5px solid #dee2e6;\n    }\n\n    .table--appointment tr:last-child td {\n        border-bottom: 0px !important;\n    }\n</style>"]}]}