ss=\"profile-widget\">\n                    <div class=\"doctor-img\">\n                        <a @click.prevent=\"\" class=\"avatar\" href=\"#\"><img :src=\"doctor.avatar\" alt=\"\"></a>\n                    </div>\n                    <div class=\"dropdown profile-action\">\n                        <a aria-expanded=\"false\" class=\"action-icon dropdown-toggle\" data-toggle=\"dropdown\" href=\"#\"><i\n                                class=\"fa fa-ellipsis-v\"></i></a>\n                        <div class=\"dropdown-menu dropdown-menu-right\">\n                            <a class=\"dropdown-item\" href=\"edit-doctor.html\"><i class=\"fa fa-pencil m-r-5\"></i> Edit</a>\n                            <a @click.prevent=\"deleteDoctor(doctor.id)\" class=\"dropdown-item\" data-target=\"#delete_doctor\"\n                               data-toggle=\"modal\" href=\"#\"><i class=\"fa fa-trash-o m-r-5\"></i>\n                                Delete</a>\n                        </div>\n                    </div>\n                    <h4 class=\"doctor-name text-ellipsis\"><a href=\"profile.html\">{{doctor.name}}</a></h4>\n                    <div class=\"doc-prof\">Gynecologist</div>\n                    <div class=\"user-country\">\n                        <i class=\"fa fa-map-marker\"></i> {{doctor.address.full_address}}\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-sm-12\">\n                <div class=\"see-all\">\n                    <a @click.prevent=\"getDoctor()\" class=\"see-all-btn\" href=\"#\" v-if=\"component.btnLoad.display\">Load\n                        More</a>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n  import API from \"../utils/api\";\n  import Utils from \"../utils/utils\";\n\n  export default {\n    name: \"AdminDoctor\",\n    data() {\n      return {\n        doctor: {\n          \"id\": 0,\n          \"name\": \"\",\n          \"email\": \"\",\n          address: {full_address: ''},\n          \"avatar\": \"\",\n          \"phone\": \"\"\n        },\n        doctors: [],\n        page: 1,\n        component: {\n          btnLoad: {\n            display: true\n          }\n        }\n      }\n    },\n    mounted() {\n      this.getDoctor(1)\n    },\n    methods: {\n      getDoctor() {\n        fetch(API.getApi(API.ADMIN.DOCTOR.LIST), {\n          method: 'post',\n          headers: {\n            'Accept': 'application/json',\n            'Content-Type': 'application/json',\n            authorization: Utils.getLocalAdmin().token\n          },\n          body: JSON.stringify({\n            page: this.page\n          })\n        }).then(res => res.json()).then(res => {\n          this.doctors = this.doctors.concat(res.data.datas);\n          if (this.page >= res.data.total_page) {\n            this.component.btnLoad.display = false;\n          }\n          this.page++;\n        }).catch(e => {\n          console.log(e)\n        });\n      },\n      deleteDoctor(id) {\n        alert(\"delete: \" + id);\n      }\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>\n"]}]}