                                         vid=\"old_password\"\n                                            name=\"Old Password\"\n                                            rules=\"required|special\"\n                                            v-slot=\"{ errors }\">\n\n                                        <div style=\"display: flex;position: relative;\">\n                                            <input id=\"old__password\" v-model=\"password.old_password\" type=\"password\" class=\"form-control\" placeholder=\"Old Password\">\n                                            <span class=\"tick--eye\"\n                                                  @click=\"onTogglePassword('old__password', 'show_pwd_old')\"><i\n                                                    :class=\"show_pwd_old ? 'fa fa-eye' : 'fa fa-eye-slash'\"></i></span>\n                                        </div>\n                                        <small class=\"text-danger text-sm-left\">{{errors[0]}}</small>\n                                    </validation-provider>\n                                </div>\n                            </div>\n                        </div>\n\n                        <div class=\"row\">\n                            <div class=\"col-sm-6\">\n                                <div class=\"form-group\">\n                                    <span>New Password</span>\n                                    <validation-provider\n                                            vid=\"new_password\"\n                                            name=\"New Password\"\n                                            rules=\"required|special\"\n                                            v-slot=\"{ errors }\">\n                                        <div style=\"display: flex;position: relative;\">\n                                            <input v-model=\"password.new_password\"\n                                                   id=\"new__password\"\n                                                   type=\"password\"\n                                                   class=\"form-control\"\n                                                   placeholder=\"New Password\">\n                                            <span class=\"tick--eye\"\n                                                  style=\"right: 20px\"\n                                                  @click=\"onTogglePassword('new__password', 'show_pwd')\"><i\n                                                    :class=\"show_pwd ? 'fa fa-eye' : 'fa fa-eye-slash'\"></i></span>\n                                            <span class=\"tick-save\"><i\n                                                    class=\"fa fa-check-circle text-muted\"></i></span>\n                                        </div>\n\n                                        <small class=\"text-danger text-sm-left\">{{errors[0]}}</small>\n                                    </validation-provider>\n                                </div>\n                            </div>\n\n                            <div class=\"col-sm-6\">\n                                <div class=\"form-group\">\n                                    <span>Confirm Password</span>\n                                    <validation-provider vid=\"new_password_confirmation\"\n                                                         name=\"Confirm Password\"\n                                                         rules=\"required|special|confirmed:new_password\"\n                                                         v-slot=\"{ errors }\">\n                                        <div style=\"display: flex;position: relative;\">\n                                            <input type=\"password\"\n                                                   v-model=\"password.new_password_confirmation\"\n                                                   id=\"new__password_confirm\"\n                                                   class=\"form-control\"\n                                                   placeholder=\"Confirm Password\">\n                                            <span class=\"tick--eye\"\n                                                  style=\"right: 20px\"\n                                                  @click=\"onTogglePassword('new__password_confirm', 'show_pws_confirm')\"><i\n                                                    :class=\"show_pws_confirm ? 'fa fa-eye' : 'fa fa-eye-slash'\"></i></span>\n                                            <span class=\"tick-save\"><i\n                                                    class=\"fa fa-check-circle text-muted\"></i></span>\n                                        </div>\n\n                                        <small class=\"text-danger text-sm-left\">{{errors[0]}}</small>\n                                    </validation-provider>\n                                </div>\n                            </div>\n                        </div>\n\n                        <div class=\"row m-t-20\">\n                            <div class=\"col-sm-3\"></div>\n                            <div class=\"col-sm-5\">\n                                <button type=\"submit\"\n                                        :disabled=\"!passed\"\n                                        class=\" btn-savepassword \">\n                                    UPDATE PASSWORD\n                                </button>\n                            </div>\n                            <div class=\"col-sm-4\"></div>\n                        </div>\n                    </form>\n                </validation-observer>\n            </div>\n            <div class=\"col-sm-2\">\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n  import rf from \"../../api/RequestFactory\";\n  import {mapGetters} from 'vuex'\n\n  export default {\n    data() {\n      return {\n        show_pwd_old: false,\n        show_pwd: false,\n        show_pws_confirm: false,\n        password: {\n          old_password: null,\n          new_password: null,\n          new_password_confirmation: null\n        }\n      }\n    },\n    methods: {\n      updatePassword() {\n        let self = this\n        let params = self.password\n\n        rf.getRequest('AuthRequest').postChangePassword(params).then(res => {\n          if (res.message) {\n            self.$store.dispatch('app/showModal', {message: res.message})\n          }\n        });\n      },\n\n      onTogglePassword(selectorId, show_pwd) {\n        let self = this\n        let x = document.getElementById(selectorId)\n        if (x.type === 'password') {\n          self[show_pwd] = true\n          x.type = 'text'\n        } else {\n          self[show_pwd] = false\n          x.type = 'password'\n        }\n      },\n    }\n  }\n</script>\n\n<style>\n    .tick--eye {\n        display: block;\n        position: absolute;\n        width: 30px;\n        line-height: 40px;\n        right: -5px;\n    }\n</style>"]}]}