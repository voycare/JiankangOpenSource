mounted() {\n      let self = this\n\n      self.id = self.$route.params.id\n      self.getDetail()\n    },\n    methods: {\n      getDetail() {\n        let self = this\n\n        rf.getRequest('AdminRequest').getTranslator(self.id).then(res => {\n          self.translator = res.data\n        })\n      },\n      onUpdate(translator) {\n        let self = this\n        let params = translator\n        params.id = self.id\n\n        rf.getRequest('AdminRequest').postTranslatorCreate(params).then(res => {\n          self.translator = res.data\n          self.$store.dispatch('app/showModal', {message: res.message})\n        })\n      }\n    }\n  }\n</script>\n<style>\n\n</style>\n"]}]}