      <div class=\"card\">\n                <div class=\"card-body\">\n                    <h3 class=\"desc-18\">Clinic Information</h3>\n\n                    <div class=\"row\">\n                        <div class=\"col-sm-3\">\n                            <AvatarEdit @onChangeAvatar=\"onChangeAvatar\" user=\"clinic\"></AvatarEdit>\n\n                            <p class=\"desc-11\">No more than 2MB max.</p>\n                            <p class=\"desc-11\">Format: JPG or PNG</p>\n                        </div>\n                        <div class=\"col-sm-9\">\n                            <div class=\"row\">\n                                <div class=\"col-sm-12\">\n                                    <div class=\"form-group\">\n                                        <label>Name of practice or clinic</label>\n                                        <input class=\"form-control\" readonly=\"true\" type=\"text\" v-model=\"clinic.name\">\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"row\">\n                                <div class=\"col-sm-6\">\n                                    <label>Type of practice or clinic</label>\n                                    <input class=\"form-control\" readonly=\"true\" type=\"text\"\n                                           v-model=\"clinic.profile.type_clinic\">\n                                </div>\n                                <div class=\"col-sm-6\">\n                                    <label for=\"\">Speciality</label>\n                                    <select class=\"form-control\" id=\"\" placeholder=\"Select\"\n                                            v-model=\"clinic.specialty_id\">\n                                        <option :key=\"id\" :value=\"id\" v-for=\"(spec, id) in appt_specialty\">{{ spec }}\n                                        </option>\n                                    </select>\n                                </div>\n                            </div>\n\n                            <div class=\"row\">\n                                <div class=\"col-sm-6\">\n                                    <label for=\"\">Email address</label>\n                                    <input class=\"form-control\" readonly type=\"email\" v-model=\"clinic.email\">\n                                </div>\n\n                                <div class=\"col-sm-6\">\n                                    <label for=\"\">Phone number</label>\n                                    <input class=\"form-control\" readonly type=\"text\" v-model=\"clinic.phone\">\n                                </div>\n                            </div>\n\n                            <div class=\"row\">\n                                <div class=\"col-sm-6\">\n                                    <label for=\"\">Contact Person</label>\n                                    <input class=\"form-control\" readonly type=\"text\" v-model=\"clinic.contact_person\">\n                                </div>\n\n                                <div class=\"col-sm-6\">\n                                    <label for=\"\">Title</label>\n                                    <input class=\"form-control\" readonly type=\"text\" v-model=\"clinic.title\">\n                                </div>\n                            </div>\n\n                            <div class=\"row\">\n                                <div class=\"col-sm-6\">\n                                    <label for=\"\">Clinic address</label>\n                                    <input class=\"form-control\" placeholder=\"Street line 1\" type=\"text\"\n                                           v-model=\"clinic.address.street_line_1\">\n                                </div>\n\n                                <div class=\"col-sm-6\">\n                                    <label for=\"\"></label>\n                                    <input class=\"form-control\" placeholder=\"Street line 2\" type=\"text\"\n                                           v-model=\"clinic.address.street_line_2\">\n                                </div>\n                            </div>\n\n                            <div class=\"row\">\n                                <div class=\"col-sm-4\">\n                                    <label for=\"\"></label>\n                                    <input class=\"form-control\" placeholder=\"City\" type=\"text\"\n                                           v-model=\"clinic.address.city\">\n                                </div>\n\n                                <div class=\"col-sm-4\">\n                                    <label for=\"\"></label>\n                                    <input class=\"form-control\" placeholder=\"State/Province\" type=\"text\"\n                                           v-model=\"clinic.address.state\">\n                                </div>\n\n                                <div class=\"col-sm-4\">\n                                    <label for=\"\"></label>\n                                    <input class=\"form-control\" placeholder=\"State/Province\" readonly\n                                           type=\"text\" v-model=\"clinic.address.country\">\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card\">\n                <div class=\"card-body\">\n                    <h3 class=\"desc-18\">Clinic Profile</h3>\n\n                    <textarea class=\"form-control\" cols=\"30\" rows=\"10\" v-model=\"clinic.profile.description\"></textarea>\n                </div>\n            </div>\n\n            <div class=\"card\">\n                <div class=\"card-body\">\n                    <h3 class=\"desc-18\">Languages Spoken</h3>\n\n                    <div class=\"form-inline\">\n                        <button :key=\"language.id\" @click=\"onClinicLanguageRemove(language.id)\" class=\"btn btn-pink mr-2\"\n                                v-for=\"language in clinic.languages\">{{ language.language }}\n                        </button>\n\n                        <select @change=\"onClinicLanguageSave\" class=\"form-control mr-2\" v-if=\"toggle_country\"\n                                v-model=\"country_selected\">\n                            <option :key=\"country.id\" :value=\"country.name\" v-for=\"country in countries\">{{ country.name\n                                }}\n                            </option>\n                        </select>\n\n                        <button @click=\"toggle_country = true\" class=\"btn btn-link\"><i class=\"fa fa-plus\"></i></button>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card\">\n                <div class=\"card-body\">\n                    <h3 class=\"desc-18\">Awards / Certifications</h3>\n\n                    <div :key=\"certificate.id\" class=\"border--bottom mt-2 mb-2\"\n                         v-for=\"(certificate, index) in clinic.certificates\">\n                        <div class=\"row\">\n                            <div class=\"col-sm-4\">\n                                <div class=\"form-group\">\n                                    <label for=\"\">Title of certificate</label>\n                                    <input class=\"form-control\" placeholder=\"Title\" type=\"text\"\n                                           v-model=\"certificate.name\">\n                                </div>\n                            </div>\n                            <div class=\"col-sm-4\">\n                                <div class=\"form-group\">\n                                    <label for=\"\"></label>\n                                    <input @change=\"takeCertificatePhoto($event, index)\" accept=\"image/*\" capture=\"environment\"\n                                           class=\"form-control\" placeholder=\"Upload certificate\"\n                                           type=\"file\">\n                                    <small>No more than 2MB max.</small>\n                                    <small>Format: JPG or PNG</small>\n                                </div>\n                            </div>\n                            <div class=\"col-sm-4\">\n                                <div class=\"certificate--thumb\">\n                                    <img :src=\"certificate.path\" v-if=\"certificate.path\">\n                                    <img class=\"no-avatar\" v-else>\n                                </div>\n                            </div>\n                        </div>\n\n                        <div class=\"row\">\n                            <div class=\"col-sm-4\">\n                                <div class=\"form-group\">\n                                    <label for=\"\">Issuer of certificate</label>\n                                    <input class=\"form-control\" placeholder=\"Issuer\" type=\"text\"\n                                           v-model=\"certificate.description\">\n                                </div>\n                            </div>\n                            <div class=\"col-sm-4\">\n\n                            </div>\n                            <div class=\"col-sm-4\">\n\n                            </div>\n                        </div>\n                    </div>\n\n                    <div class=\"row\">\n                        <div class=\"col-sm-12 text-right\">\n                            <button @click=\"onAddCertificate\" class=\"btn btn-link\"><i class=\"fa fa-plus\"></i>Add\n                                Certificate\n                            </button>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"text-center\">\n                <button @click=\"onSave\" class=\"btn btn-pink btn-round btn-lg\">Save</button>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n  import {mapGetters} from 'vuex'\n\n  import Const from \"./../../utils/const\"\n  import rf from \"../../api/RequestFactory\"\n  import AvatarEdit from '../../components/AvatarEdit'\n\n  export default {\n    components: {\n      AvatarEdit\n    },\n    data() {\n      return {\n        Const,\n        id: null,\n        clinic: null,\n        imgSrc: '',\n        toggle_country: false,\n        country_selected: null\n      }\n    },\n    computed: {\n      ...mapGetters(['countries', 'appt_specialty'])\n    },\n    methods: {\n      onClinicLanguageRemove(id) {\n        let self = this\n\n        let r = confirm(\"Are you want to remove this clinic?\");\n        if (r == true) {\n          rf.getRequest('AdminRequest').postClinicLanguageRemove(id, {}).then(res => {\n            self.$store.dispatch('app/showMessage', {message: res.message})\n\n            self.getDetail()\n          })\n        }\n      },\n      onClinicLanguageSave() {\n        let self = this\n        const params = {\n          language: self.country_selected.trim()\n        }\n\n        rf.getRequest('AdminRequest').postClinicLanguageUpdate(self.id, params).then(res => {\n          self.$store.dispatch('app/showMessage', {message: res.message})\n\n          self.getDetail()\n          self.country_selected = null\n          self.toggle_country = false\n        })\n      },\n      onSave() {\n        let self = this\n        const params = {\n          clinic: self.clinic\n        }\n\n        rf.getRequest('AdminRequest').postClinicUpdate(self.id, params).then(res => {\n          self.$store.dispatch('app/showMessage', {message: res.message})\n          self.clinic = res.data\n        })\n      },\n      onChangeAvatar(imgSrc) {\n        this.imgSrc = imgSrc\n      },\n      getDetail() {\n        let self = this\n        rf.getRequest('AdminRequest').getClinic(self.id).then(res => {\n          self.clinic = res.data\n        })\n      },\n      takeCertificatePhoto(e, index) {\n        let self = this\n        const file = e.target.files[0]\n        const reader = new FileReader()\n        reader.onload = (event) => {\n          self.clinic.certificates[index].path = event.target.result\n        }\n        reader.readAsDataURL(file);\n      },\n      onAddCertificate() {\n        let self = this\n\n        if (self.clinic) {\n          self.clinic.certificates.push({\n            name: '',\n            description: '',\n            clinic_id: self.clinic.id,\n            path: ''\n          })\n        }\n      }\n    },\n    mounted() {\n      let self = this\n\n      self.id = parseInt(this.$route.params.id)\n\n      self.$store.dispatch('getCountries')\n      self.$store.dispatch('getAppointmentMetas')\n      self.getDetail()\n    }\n  }\n</script>\n\n<style scoped>\n</style>\n"]}]}