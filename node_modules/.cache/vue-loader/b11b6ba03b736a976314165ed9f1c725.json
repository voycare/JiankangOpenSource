            <img :src=\"VUE_APP_BASE_URL + clinic.avatar\" alt=\"\" class=\"img-fluid\">\n                    </div>\n                    <div class=\"col-sm-9\">\n                        <div class=\"row\">\n                            <div class=\"col-sm-3\">\n                                <div class=\"title-22\">{{ clinic.name }}</div>\n                                <small>{{ clinic.profile.clinic_type }}</small>\n                            </div>\n                            <div class=\"col-sm-3\">{{ clinic.country }}</div>\n                            <div class=\"col-sm-3\">{{ clinic.id }}</div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"border--bottom\"></div>\n                <div class=\"title-22\">Clinic Information</div>\n                <div class=\"row\">\n                    <div class=\"col-sm-6\">\n                        <ul class=\"list-unstyled\">\n                            <li>Email address: {{ clinic.email }}</li>\n                            <li>Phone number: {{ clinic.phone }}</li>\n                            <li>Clinic address: {{ clinic.address.full_address }}</li>\n                        </ul>\n                    </div>\n                    <div class=\"col-sm-6\">\n                        <ul class=\"list-unstyled\">\n                            <li>Contact person: {{ clinic.profile.contact_person }}</li>\n                            <li>Title: {{ clinic.profile.name }}</li>\n                        </ul>\n                    </div>\n                </div>\n                <div class=\"border--bottom\"></div>\n                <div class=\"row\">\n                    <div class=\"col-md-12\">\n                        <div class=\"title-22\">Clinic Profile</div>\n                        <p>{{ clinic.profile.description }}</p>\n                    </div>\n                </div>\n                <div class=\"border--bottom\"></div>\n                <div class=\"row\">\n                    <div class=\"col-md-12\">\n                        <div class=\"title-22\">Languages Spoken</div>\n                        <ul class=\"list-inline\">\n                            <li :key=\"language.id\" class=\"btn btn-pink btn-link\"\n                                v-for=\"language in clinic.languages\">{{ language.language }}\n                            </li>\n                        </ul>\n                    </div>\n                </div>\n                <div class=\"border--bottom\"></div>\n                <div class=\"row\">\n                    <div class=\"col-md-12\">\n                        <div class=\"titlte-12\">Awards / Certifications</div>\n                        <div :key=\"certificate.id\" class=\"border--all\" v-for=\"certificate in clinic.certificates\">\n                            <div class=\"row\">\n                                <div class=\"col-sm-4\">\n                                    <img :src=\"VUE_APP_BASE_URL + certificate.path\" alt=\"\">\n                                </div>\n                                <div class=\"col-sm-8\">\n                                    <div class=\"desc-12\">{{ certificate.name }}</div>\n                                    <div class=\"desc-11\">{{ certificate.description }}</div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n  import {mapGetters} from 'vuex'\n\n  import Const from \"./../../utils/const\"\n  import rf from \"../../api/RequestFactory\"\n  import Pagination from \"../../common/Pagination\"\n  import AdminClinicListing from '../../components/AdminClinicListing'\n\n  export default {\n    components: {\n      Pagination,\n      AdminClinicListing\n    },\n    data() {\n      return {\n        Const,\n        id: null,\n        clinic: null\n      }\n    },\n    computed: {\n      ...mapGetters(['countries', 'appt_types', 'appt_status'])\n    },\n    methods: {\n      getDetail(page) {\n        let self = this\n        rf.getRequest('AdminRequest').getClinic(self.id).then((res) => {\n          self.clinic = res.data\n        });\n      },\n      onSortCreated() {\n        this.getClinicApplicants(1);\n      }\n    },\n\n    mounted() {\n      let self = this\n\n      self.id = self.$route.params.id\n\n      self.$store.dispatch('getCountries')\n      self.$store.dispatch('getAppointmentMetas')\n      self.getDetail(self.id);\n    }\n  }\n</script>\n"]}]}