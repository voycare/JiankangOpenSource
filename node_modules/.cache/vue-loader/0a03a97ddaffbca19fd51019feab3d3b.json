     return {\n        id: null,\n        services: [],\n        treatments: []\n      }\n    },\n    methods: {\n      getClinicServices() {\n        let self = this\n        rf.getRequest('ClinicRequest').getServices().then((res) => {\n          self.services = res.data.services\n          self.treatments = res.data.treatments\n          if (!self.services.length) {\n            self.services.push({\n              clinic_id: self.id,\n              type: 1,\n              duration: null,\n              price: null\n            })\n            self.services.push({\n              clinic_id: self.id,\n              type: 2,\n              duration: null,\n              price: null\n            })\n          }\n\n          if (!self.treatments.length) {\n            self.treatments.push({\n              clinic_id: self.id,\n              type: '',\n              from: '',\n              description: ''\n            })\n          }\n        });\n      },\n\n      onSaveServices(services, treatments) {\n        let self = this\n\n        let params = {\n          services,\n          treatments\n        }\n\n        rf.getRequest('ClinicRequest').postServices(params).then(res => {\n        })\n      }\n    },\n    mounted() {\n      let self = this\n\n      self.id = self.$route.params.id\n      self.getClinicServices();\n    }\n  }\n</script>\n"]}]}