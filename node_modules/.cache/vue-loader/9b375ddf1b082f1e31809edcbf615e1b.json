URL + src)\n      },\n      takePhoto(e) {\n        let self = this\n        const file = e.target.files[0];\n        const reader = new FileReader();\n        reader.onload = (event) => {\n          self.imgSrc = event.target.result;\n          self.$refs.avatar.src = event.target.result;\n\n          self.$emit('onChangeAvatar', self.imgSrc)\n        };\n        reader.readAsDataURL(file);\n      },\n      chooseImage() {\n        this.$refs.file.click();\n      },\n    }\n  }\n</script>\n\n<style>\n  .avatar-user {\n    position: relative;\n  }\n\n  .avatar-user img {\n    width: 100%;\n    height: 170px;\n  }\n\n  .avatar-user button {\n    position: absolute;\n    right: 0;\n    bottom: 0;\n    width: 54px;\n    height: 31px;\n    background: #434343;\n    font-family: Poppins;\n    font-style: normal;\n    font-weight: normal;\n    font-size: 15px;\n    line-height: 22px;\n    border: none;\n    color: #FFFFFF;\n  }\n</style>\n"]}]}